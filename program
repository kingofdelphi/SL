function mod(a, b) {
    for (;a >= b;) {
        a = a - b
    }
    return a
}

function main() {
    2 * 3
    4 * 8
    def a, pow, b
    a = 3
    pow = 1
    if (a = b = 1 <= 1.1) {
        a = a * 3
        if (a < 10) {
            def hacker
            hacker = 23
        }
        for (a=1;a<=3; a = a + 1) {
            pow = pow * 3
        }
        def x, y, z
        x = 3
        z = 12
        y = 3
    }
    def y, s, i
    y = 2
    pow = "hack"
