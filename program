function ba(chicken, pox) {
    print(chicken)
    print(" squared  = ")
    print(pox)
    print("\n")
}


result = 1

function fibon(n) {
    if (n < .1) result = 1
    if (n < 1.1) result = 1
    if (n > 1) {
        fibon(n - 2)
        a = result
        fibon(n - 1)
        result = result + a
    }
}

function factorial(x) {
    if (0.1 > x) result = 1
    if (x > 0) factorial(x - 1), result = result * x
}

function looper() {
    for (a = 1; a <= 10; a = a + 1) {
        factorial(a), print(result), print("\n")
    }
}

main()
